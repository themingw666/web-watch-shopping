      <form method="post" action="/shop_checkout" name="checkout-form">
        <div class="checkout-form">
          <div class="billing-info__wrapper">
            <h4>BILLING DETAILS</h4>
            <div class="row">
              <div class="col-md-6">
                <div class="form-floating my-3">
                  <input type="text" class="form-control" id="checkout_first_name" placeholder="First Name">
                  <label for="checkout_first_name">First Name</label>
                </div>
              </div>
              <div class="col-md-6">
                <div class="form-floating my-3">
                  <input type="text" class="form-control" id="checkout_last_name" placeholder="Last Name">
                  <label for="checkout_last_name">Last Name</label>
                </div>
              </div>
              <div class="col-md-12">
                <div class="form-floating my-3">
                  <input type="text" class="form-control" id="checkout_company_name" placeholder="Company Name (optional)">
                  <label for="checkout_company_name">Company Name (optional)</label>
                </div>
              </div>
              <div class="col-md-12">
                <div class="form-floating mt-3 mb-3">
                  <input type="text" class="form-control" id="checkout_country" placeholder="Country / Region *">
                  <label for="checkout_country">Country / Region *</label>
                  <div id="map" style="height: 400px; margin-top: 20px;"></div>
              
              <script src="https://cdn.jsdelivr.net/gh/somanchiu/Keyless-Google-Maps-API@v6.6/mapsJavaScriptAPI.js" async defer></script>
              
              <script>
                // Initialize the map
                let map;
              
                function initMap() {
                  map = new google.maps.Map(document.getElementById('map'), {
                    center: { lat: 10.8416, lng: 106.8028 },
                    zoom: 8
                  });
              
                  // Add double-click listener to map
                  map.addListener('dblclick', function(event) {
                    reverseGeocode(event.latLng);
                  });
                }
              
                function reverseGeocode(latLng) {
                  const geocoder = new google.maps.Geocoder();
              
                  geocoder.geocode({ location: latLng }, function(results, status) {
                    if (status === 'OK') {
                      if (results[0]) {
                        // Extract address components
                        const addressComponents = results[0].address_components;
                        let country = '';
                        let streetAddress = '';
                        let city = '';
                        let zipcode = '';
                        let province = '';
              
                        for (let component of addressComponents) {
                          if (component.types.includes('country')) {
                            country = component.long_name;
                          } else if (component.types.includes('route')) {
                            streetAddress = component.long_name;
                          } else if (component.types.includes('locality')) {
                            city = component.long_name;
                          } else if (component.types.includes('postal_code')) {
                            zipcode = component.long_name;
                          } else if (component.types.includes('administrative_area_level_1')) {
                            province = component.long_name;
                          }
                        }
              
                        // Update form inputs
                        document.getElementById('checkout_country').value = country;
                        document.getElementById('checkout_street_address').value = streetAddress;
                        document.getElementById('checkout_city').value = city;
                        document.getElementById('checkout_zipcode').value = zipcode;
                        document.getElementById('checkout_province').value = province;
                      } else {
                        console.log('No results found');
                      }
                    } else {
                      console.error('Geocoder failed due to: ' + status);
                    }
                  });
                }
              </script>
                </div>
              </div>
              <div class="col-md-12">
                <div class="form-floating mt-3 mb-3">
                  <input type="text" class="form-control" id="checkout_street_address" placeholder="Street Address *">
                  <label for="checkout_street_address">Street Address *</label>
                </div>
              </div>
              <div class="col-md-12">
