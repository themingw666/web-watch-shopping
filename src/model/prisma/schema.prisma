generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id        Int        @id @default(autoincrement())
  username  String     @unique
  email     String     @unique
  password  String
  role      Int
  money     BigInt
  products  products[]
}

model products {
  id        Int        @id @default(autoincrement())
  name      String
  money     Int 
  quantity  Int
  des       String
  authorid  Int
  image     String
  author    users       @relation(fields: [authorid], references: [id])
}